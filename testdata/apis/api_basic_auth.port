Get("$TEST_URL")
BasicAuth("user", "pass")
ResponseType("json")

LoadStrategy(Full)
TableDefinition({
	"id": "INT",
	"name": "VARCHAR(255)",
})

def Paginate(previous_response):
	return None

def Transform(data):
  return [{ 
    'id': item['id'],
    'name': item['name'],
    'created_at': time.fromtimestamp(int(item['created_at'])),
  } for item in data["items"]]

ErrorHandling({
	NetworkError: Retry,
	Http4XXError: Fail,
	Http5XXError: Retry,
	InvalidBodyError: Fail,
})

